    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/schedule": "^1.0.1",
    "@nestjs/testing": "^8.0.0",
    "@nestjs/typeorm": "^8.0.2",
    "@sendgrid/helpers": "^7.5.0",
    "@sendgrid/mail": "^7.5.0",
    "@types/passport-http-bearer": "^1.0.37",
    "apollo-server-errors": "^3.1.0",
