    "class-validator": "^0.13.1",
    "crypto": "^1.0.1",
    "date-fns": "^2.25.0",
    "date-fns-tz": "^1.2.2",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "gql-query-builder": "^3.5.5",
    "graphql": "^15.5.3",
    "graphql-tools": "^8.2.0",
